if choice == "start":
        print("\nSelect the operation you want to perform in the calculator:")
        print("1. Arithmetic operator")
        print("2. Relational operator")
        print("3. Logical operator")
        print("4. Bitwise operator")
        print("5. Assignment operator")
        print("6. Identity operator")
        print("7. Membership operator")
    
        operation = int(input("\nEnter the choice (1-7): "))
    
        if operation == 1:
            print("\nIn arithmetic operator what do you want to perform?")
            print("1. Addition")
            print("2. Subtraction")
            print("3. Multiplication")
            print("4. Division")
            print("5. Modulus")
            print("6. Exponential")
            arithmetic_choice = int(input("\nEnter the choice: "))
    
            if arithmetic_choice == 1:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                print("Addition is", a + b)
    
            elif arithmetic_choice == 2:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                print("Subtraction is", a - b)
    
            elif arithmetic_choice == 3:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                print("Multiplication is", a * b)
    
            elif arithmetic_choice == 4:
                print("There are two types of division:")
                print("1. Float division")
                print("2. Floor division")
                division_choice = int(input("\nEnter the choice: "))
    
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
    
                if division_choice == 1:
                    print("Float division is", a / b)
                elif division_choice == 2:
                    print("Floor division is", a // b)
    
            elif arithmetic_choice == 5:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                print("Modulus is", a % b)
    
            elif arithmetic_choice == 6:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                print("Exponential is", a ** b)
    
        elif operation == 2:
            print("\nIn relational operator what do you want to perform?")
            print("1. Greater than (>)")
            print("2. Less than (<)")
            print("3. Equals to (==)")
            print("4. Not equal to (!=)")
            print("5. Greater than or equal to (>=)")
            print("6. Less than or equal to (<=)")
            relational_choice = int(input("\nEnter your choice: "))
    
            a = int(input("Enter the first number: "))
            b = int(input("Enter the second number: "))
    
            if relational_choice == 1:
                print("Greater than is", a > b)
    
            elif relational_choice == 2:
                print("Less than is", a < b)
    
            elif relational_choice == 3:
                print("Equal to is", a == b)
    
            elif relational_choice == 4:
                print("Not equal to is", a != b)
    
            elif relational_choice == 5:
                print("Greater than or equal to is", a >= b)
    
            elif relational_choice == 6:
                print("Less than or equal to is", a <= b)
    
        elif operation == 3:
            print("\nIn logical operator what do you want to perform?")
            print("1. AND")
            print("2. OR")
            print("3. NOT")
            logical_choice = int(input("\nEnter the choice: "))
    
            a = int(input("Enter the first number: "))
            b = int(input("Enter the second number: "))
    
            if logical_choice == 1:
                print("AND operator result is", a and b)
    
            elif logical_choice == 2:
                print("OR operator result is", a or b)
    
            elif logical_choice == 3:
                print("NOT operator for the first number is", not a)
                print("NOT operator for the second number is", not b)
    
        elif operation == 4:
            print("\nIn bitwise operator what do you want to perform?")
            print("1. Bitwise AND (&)")
            print("2. Bitwise OR (|)")
            print("3. Bitwise XOR (^)")
            print("4. Bitwise NOT (~)")
            print("5. Bitwise Right shift (>>)")
            print("6. Bitwise Left shift (<<)")
            bitwise_choice = int(input("\nEnter the choice: "))
    
            a = int(input("Enter the first number: "))
            b = int(input("Enter the second number: "))
    
            if bitwise_choice == 1:
                print("Bitwise AND operator is", a & b)
    
            elif bitwise_choice == 2:
                print("Bitwise OR operator is", a | b)
    
            elif bitwise_choice == 3:
                print("Bitwise XOR operator is", a ^ b)
    
            elif bitwise_choice == 4:
                print("Bitwise NOT operator is", ~a)
    
            elif bitwise_choice == 5:
                n = int(input("Enter the number of positions to right shift: "))
                print("Bitwise Right shift is", a >> n)
    
            elif bitwise_choice == 6:
                n = int(input("Enter the number of positions to left shift: "))
                print("Bitwise Left shift is", a << n)
    
        elif operation == 5:
            print("\nIn assignment operator what do you want to perform?")
            print("1. Assignment (=)")
            print("2. Addition Assignment (+=)")
            print("3. Subtraction Assignment (-=)")
            print("4. Multiplication Assignment (*=)")
            print("5. Division Assignment (/=)")
            assignment_choice = int(input("\nEnter the choice: "))
    
            if assignment_choice == 1:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                c = a + b
                print("Assignment operator result is", c)
    
            elif assignment_choice == 2:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                a += b
                print("Addition Assignment operator result is", a)
    
            elif assignment_choice == 3:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                a -= b
                print("Subtraction Assignment operator result is", a)
    
            elif assignment_choice == 4:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                a *= b
                print("Multiplication Assignment operator result is", a)
    
            elif assignment_choice == 5:
                a = int(input("Enter the first number: "))
                b = int(input("Enter the second number: "))
                a /= b
                print("Division Assignment operator result is", a)
    
        elif operation == 6:
            print("\nIn identity operator what do you want to perform?")
            print("1. is")
            print("2. is not")
            identity_choice = int(input("\nEnter the choice: "))
    
            a = int(input("Enter the first number: "))
            b = int(input("Enter the second number: "))
    
            if identity_choice == 1:
                print(a is b)
    
            elif identity_choice == 2:
                print(a is not b)
    
        elif operation == 7:
            print("\nIn membership operator what do you want to perform?")
            print("1. in")
            print("2. not in")
            membership_choice = int(input("\nEnter the choice: "))
    
            if membership_choice == 1:
                name = input("Enter the name: ")
                word = input("Enter the word you want to find: ")
                print(word in name)
    
            elif membership_choice == 2:
                name = input("Enter the name: ")
                word = input("Enter the word to find: ")
                print(word not in name)
    
    else:
        print("Exiting the program.")
        break
